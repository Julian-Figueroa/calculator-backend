{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "DATABASE_URL": "@dev-db"
  },
  "envSecrets": [
    "DATABASE_URL"
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/users",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "methods": ["GET", "POST"],
      "dest": "index.js"
    },
    {
      "src": "/new-operation",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/user/:userId/records",
      "methods": ["GET"],
      "dest": "index.js"
    },
    {
      "src": "/user/records",
      "methods": ["DELETE"],
      "dest": "index.js"
    }
  ]
}
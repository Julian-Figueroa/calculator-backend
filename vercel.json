{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/calculate",
      "dest": "index.js"
    },
    {
      "src": "/users",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "methods": ["GET", "POST"],
      "dest": "index.js"
    },
    {
      "src": "/new-operation",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/user/:userId/records",
      "methods": ["GET"],
      "dest": "index.js"
    },
    {
      "src": "/user/records",
      "methods": ["DELETE"],
      "dest": "index.js"
    }
  ],
  "public": "/public"
}
